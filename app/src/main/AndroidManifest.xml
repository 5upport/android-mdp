<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.infor2018.biodatadirisqlite">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Biodatadirisqlite">
        <activity android:name="com.infor2018.biodatadirisqlite.UpdateBiodata"></activity>
        <activity android:name="com.infor2018.biodatadirisqlite.LihatBiodata" />
        <activity android:name="com.infor2018.biodatadirisqlite.BuatBiodata" />
        <activity android:name="com.infor2018.biodatadirisqlite.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>